leaveOneOut = function(datosX, datosY, h){
error = 0
for (i in (1: length(datosX))){
predichoI = promediosMoviles(datosX[-i], datosY[-i], datosX[i], h)
error = error + abs(predichoI - datosY[i])
}
return(error)
}
